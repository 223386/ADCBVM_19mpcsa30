import java.util.*;
class update
{
  public static void main(String args[])
  {
    Scanner sc=new Scanner (System.in);
    int n;
    ArrayList<Integer>x=new ArrayList<Integer>();
    System.out.println("Enter n value");
    n=sc.nextInt();
    for(int i=0;i<=n;i++)
    {
      x.set(2,10);
      }
      }
      }
Output
Enter n value 
4
10
30
40
50
10,30,10,50
